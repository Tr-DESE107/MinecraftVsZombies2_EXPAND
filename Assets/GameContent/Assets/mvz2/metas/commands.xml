<commands>
    <command id="help">
        <description>显示帮助界面。</description>
        <variant>
            <description>显示帮助界面。</description>
            <params/>
        </variant>
        <variant>
            <description>查询命令的作用。</description>
            <params>
                <param name="command" type="command">命令名。</param>
            </params>
        </variant>
    </command>
    <command id="spawn" inLevel="true">
        <description>生成一个实体。</description>
        <variant subname="pos">
            <description>生成一个实体。默认位置为场地中心。</description>
            <params>
                <param name="id" type="id" idType="entity">要生成的实体ID。</param>
                <param name="x" type="float" optional="true">可选。要生成的实体位置的X值。</param>
                <param name="y" type="float" optional="true">可选。要生成的实体位置的Y值。</param>
                <param name="z" type="float" optional="true">可选。要生成的实体位置的Z值。</param>
            </params>
        </variant>
        <variant subname="grid">
            <description>在指定的地格位置生成实体。默认位置为场地中心。</description>
            <params>
                <param name="id" type="id" idType="entity">要生成的实体ID。</param>
                <param name="column" type="float" optional="true">可选。实体所在列。0代表最左侧一列，向右递增。</param>
                <param name="lane" type="float" optional="true">可选。实体所在行。0代表最上方一行，向下递增。</param>
            </params>
        </variant>
    </command>
    <command id="kill" inLevel="true">
        <description>杀死实体。</description>
        <variant>
            <description>杀死所有怪物。</description>
            <params/>
        </variant>
        <variant subname="contraption">
            <description>杀死所有器械。</description>
            <params/>
        </variant>
        <variant subname="enemy">
            <description>杀死所有怪物。</description>
            <params/>
        </variant>
        <variant subname="obstacle">
            <description>杀死所有障碍物。</description>
            <params/>
        </variant>
        <variant subname="boss">
            <description>杀死所有头目。</description>
            <params/>
        </variant>
        <variant subname="all">
            <description>杀死所有实体。</description>
            <params/>
        </variant>
    </command>
    <command id="blueprint" inLevel="true">
        <description>调整蓝图。</description>
        <variant subname="set">
            <description>将某槽位的蓝图替换为指定ID的蓝图。</description>
            <params>
                <param name="slot" type="int">蓝图槽位，0代表第一个槽位，1代表第二个槽位，依此类推。</param>
                <param name="id" type="id" idType="blueprint">蓝图ID。</param>
            </params>
        </variant>
        <variant subname="remove">
            <description>移除指定槽位的蓝图。</description>
            <params>
                <param name="slot" type="int">蓝图槽位，0代表第一个槽位，1代表第二个槽位，依此类推。</param>
            </params>
        </variant>
    </command>
    <command id="energy" inLevel="true">
        <description>调整当前能量。</description>
        <variant subname="set">
            <description>设置当前能量。</description>
            <params>
                <param name="value" type="float">能量。</param>
            </params>
        </variant>
        <variant subname="add">
            <description>增加或减少当前能量。</description>
            <params>
                <param name="value" type="float">能量。</param>
            </params>
        </variant>
    </command>
    <command id="starshard" inLevel="true">
        <description>调整当前星之碎片数量。</description>
        <variant subname="set">
            <description>设置当前星之碎片数量。</description>
            <params>
                <param name="value" type="int">星之碎片数量。</param>
            </params>
        </variant>
        <variant subname="add">
            <description>增加或减少当前星之碎片数量。</description>
            <params>
                <param name="value" type="int">星之碎片数量。</param>
            </params>
        </variant>
    </command>
    <command id="recharge" inLevel="true">
        <description>立即充能所有蓝图。</description>
        <variant>
            <description>立即充能所有蓝图。</description>
            <params/>
        </variant>
    </command>
    <command id="cheat" inLevel="true">
        <description>启用关卡中的作弊选项。再次使用后关闭该效果。</description>
        <variant subname="recharge">
            <description>所有蓝图无需充能。</description>
            <params/>
        </variant>
        <variant subname="starshard">
            <description>无限星之碎片。</description>
            <params/>
        </variant>
        <variant subname="godmode">
            <description>不会游戏结束。</description>
            <params/>
        </variant>
    </command>
    <command id="repeat">
        <description>重复上一条指令。</description>
        <variant>
            <description>重复上一条指令。</description>
            <params>
                <param name="times" type="int">次数。</param>
            </params>
        </variant>
    </command>
    <command id="clear">
        <description>清屏。</description>
        <variant>
            <description>清屏。</description>
            <params/>
        </variant>
    </command>
    <command id="clearlevel" inLevel="true">
        <description>生成一个通关掉落物。</description>
        <variant>
            <description>生成一个通关掉落物。</description>
            <params/>
        </variant>
    </command>
    <command id="save" inLevel="true">
        <description>保存当前关卡的进度。</description>
        <variant>
            <description>保存当前关卡的进度。</description>
            <params/>
        </variant>
    </command>
    <command id="load" inLevel="true">
        <description>重新读取当前关卡的进度。</description>
        <variant>
            <description>重新读取当前关卡的进度。</description>
            <params/>
        </variant>
    </command>
    <command id="spawnblueprint" inLevel="true">
        <description>生成指定蓝图掉落物。</description>
        <variant subname="pos">
            <description>生成指定蓝图掉落物。默认位置为场地中心。</description>
            <params>
                <param name="id" type="id" idType="blueprint">要生成的蓝图掉落物的对应实体ID。</param>
                <param name="x" type="float" optional="true">可选。要生成的掉落物位置的X值。</param>
                <param name="y" type="float" optional="true">可选。要生成的掉落物位置的Y值。</param>
                <param name="z" type="float" optional="true">可选。要生成的掉落物位置的Z值。</param>
            </params>
        </variant>
        <variant subname="grid">
            <description>在指定的地格位置生成指定蓝图掉落物。默认位置为场地中心。</description>
            <params>
                <param name="id" type="id" idType="blueprint">要生成的蓝图掉落物的对应实体ID。</param>
                <param name="column" type="float" optional="true">可选。掉落物所在列。0代表最左侧一列，向右递增。</param>
                <param name="lane" type="float" optional="true">可选。掉落物所在行。0代表最上方一行，向下递增。</param>
            </params>
        </variant>
    </command>
    <command id="artifact" inLevel="true">
        <description>调整制品。</description>
        <variant subname="set">
            <description>将某槽位的制品替换为指定ID的制品。</description>
            <params>
                <param name="slot" type="int">制品槽位，0代表第一个槽位，1代表第二个槽位，依此类推。</param>
                <param name="id" type="id" idType="artifact">制品ID。</param>
            </params>
        </variant>
        <variant subname="remove">
            <description>移除指定槽位的制品。</description>
            <params>
                <param name="slot" type="int">制品槽位，0代表第一个槽位，1代表第二个槽位，依此类推。</param>
            </params>
        </variant>
    </command>
</commands>