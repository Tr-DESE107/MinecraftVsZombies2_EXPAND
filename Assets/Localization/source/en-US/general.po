msgid "显示帮助界面。"
msgstr "Displays the command list."

#. Description for command variant "mvz2:help "
#: Commands meta file
msgctxt "command_variant.description"
msgid "显示帮助界面。"
msgstr "Displays the command list."

#. Description for command variant "mvz2:help "
#: Commands meta file
